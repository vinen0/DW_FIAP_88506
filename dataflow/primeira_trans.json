{
	"name": "primeira_trans",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "rawcp",
						"type": "DatasetReference"
					},
					"name": "Loja"
				},
				{
					"dataset": {
						"referenceName": "rawcp",
						"type": "DatasetReference"
					},
					"name": "end"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "rawcp",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          NR_LOJA as short,",
				"          NM_LOJA as string,",
				"          DT_INAUGURACAO as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Loja",
				"source(output(",
				"          CD_LOJA_END as short,",
				"          NR_LOJA as short,",
				"          CD_LOGRADOURO as short,",
				"          NR_END as short,",
				"          DS_COMPLEMENTO_END as string,",
				"          DT_INICIO as string,",
				"          DT_TERMINO as string,",
				"          ST_END as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> end",
				"Loja, end join(Loja@NR_LOJA == end@NR_LOJA,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}